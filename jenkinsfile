pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
                echo 'welcome to jenkins'
                echo 'its done'
            }
        }
        stage('build'){
            steps{
                echo 'building'
            }
        }
        stage('deploy'){
            steps{
                echo 'deploying'
            }
        }
        stage('test'){
            steps{
                echo 'testing'
            }
        }
    }
}
